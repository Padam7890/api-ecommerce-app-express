{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
      "prisma-migrate": {
        "handler": "index.prismamigrate",
        "memory": 512
      },
      "prisma-generate": {
        "handler": "index.prismagenerate",
        "memory": 512
      }
    }
  }
  